{
  "name": "@eagle/tango",
  "version": "1.2.1",
  "description": "Toolkit for building redux/react applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/Graf009/tango.git"
  },
  "author": "Oleg Orlov <mail@orlovoleg.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run check && npm run build",
    "clean": "rimraf dist es",
    "lint": "eslint src test",
    "test": "cross-env BABEL_ENV=commonjs ava",
    "check": "npm run lint && npm run test",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "@eagle/redux-immutablejs": "0.1.0",
    "babel-runtime": "6.9.2",
    "classnames": "2.2.5",
    "hoist-non-react-statics": "1.2.0",
    "immutable": "3.8.1",
    "lodash.filter": "4.4.0",
    "lodash.foreach": "4.3.0",
    "lodash.isfunction": "3.0.8",
    "lodash.map": "4.4.0",
    "lodash.mapvalues": "4.4.0",
    "lodash.reduce": "4.4.0",
    "lodash.values": "4.1.0",
    "react-immutable-proptypes": "1.7.1",
    "react-redux": "4.4.5",
    "reduce-reducers": "0.1.2",
    "redux": "3.5.2",
    "redux-actions": "0.10.1",
    "redux-batched-subscribe": "0.1.6",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "@eagle/eslint-config-eagle": "1.2.4",
    "ava": "0.15.2",
    "babel-cli": "6.11.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-es2015-loose-native-modules": "1.0.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "cross-env": "2.0.0",
    "eslint": "2.13.1",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.2.2",
    "eslint-plugin-react": "5.1.1",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "rimraf": "2.5.3"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
